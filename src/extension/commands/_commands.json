{

    //--- SUBMENU: assignIconSubMenu -------------------------------------------------------------------->>
    "assignIconSubmenu": {
        "label": "FT-UI: Assign Icon",
        "location": "explorer/context",
        "group": "2_workspace@0"
    },

    //- Commands for AssignIcon submenu ----------
    "assignIcon": {
        "name": "Use Alternate icon",
        "submenu": "assignIconSubmenu"
    },

    "revertIcon": {
        "name": "Use Default Icon",
        "submenu": "assignIconSubmenu"
    },

    "showAvailableIcons": {
        "name": "Show Available Icons",
        "submenu": "assignIconSubmenu", "subgroup":2
    },

    "showFolderIconAssignment": {
        "name": "Show Folder Assignments",
        "submenu": "assignIconSubmenu", "subgroup":2
    },
    //---------------------------------------------------------------------------------------------------<<

    
    //- Editor/Context ------------------------------------------------------------ 
    
    "debug.logMessage": { 
        "name": "Log Message",
        "menu": { "name": "editor/context", "group": "navigation@10" } // TODO Fix why this outputs "group": "navigation@101",
    },
    
    
    
    //- Command Palette only ------------------------------------------------------ 

    "toggleIcons":          { "name": "Toggle Icons",           "group": "Icons" },
    "activateIconTheme":    { "name": "Activate Icon Theme",    "group": "Icons" },
    "toggleExplorerArrows": { "name": "Toggle Explorer Arrows", "group": "Icons" }

    
    
}



