{"version": "0.2.0", "configurations": [{
	"name": "Webpack - Debug",
	"type": "extensionHost",
	"request": 		 "launch",
	"preLaunchTask": "${defaultBuildTask}",
	"args": [
		"--profile=Clean",
		"--extensionDevelopmentPath=${workspaceFolder}", 
	],
	"internalConsoleOptions": "openOnSessionStart",
 	"outFiles": ["${workspaceFolder}/dist/extension.js"],
	"sourceMaps": true
}]}






